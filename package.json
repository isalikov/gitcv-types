{
  "name": "@isalikov/gitcv-types",
  "version": "0.0.1",
  "description": "gitcv.io types and interfaces",
  "main": "",
  "types": "index.d.ts",
  "repository": "git@github.com:isalikov/gitcv-types.git",
  "author": "Iakov Salikov",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "lint": "eslint ./ --ext .ts",
    "build": "tsc",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix",
      "tsc"
    ]
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    }
  },
  "eslintIgnore": [
    "node_modules"
  ],
  "prettier": {
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 4
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.35.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.4",
    "tsc": "^2.0.4",
    "typescript": "^4.9.5"
  }
}
